{
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [ "tests" ],
    "python.analysis.typeCheckingMode": "off",
    "[markdown]": {},
    "markdownlint.config": { "MD013": false,"MD024": false,"MD033": false },
    "[json]": {
        "breadcrumbs.showArrays": true,
        "editor.quickSuggestions": { "strings": true },
        "editor.suggest.insertMode": "replace",
        "editor.defaultFormatter": "GamemodeOne.json-compact-prettier",
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "file"
    },
    "[jsonc]": { "editor.defaultFormatter": "GamemodeOne.json-compact-prettier" },
    "json-compact-prettier.maxLineLength": 120,
	"json-compact-prettier.alwaysOpenKeys": [
		"alternates",
		"attendees",
		"permissions",
		"allow",
		"deny",
		"ask",
		"scripts"
	],
	"bookmarks.useWorkaroundForFormatters": true,
	"markdown.preview.breaks": false,
	"markdown-preview-github-styles.colorTheme": "auto",
	"prettier.printWidth": 100,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.formatOnSaveMode": "modifications"
}