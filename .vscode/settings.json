{
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [ "tests" ],
    "python.analysis.typeCheckingMode": "off",
    "[markdown]": {},
    "markdownlint.config": { "MD022": false, "MD032": false, "MD033": false, "MD041": false },
    "[json]": {
        "breadcrumbs.showArrays": true,
        "editor.quickSuggestions": { "strings": true },
        "editor.suggest.insertMode": "replace",
        "editor.defaultFormatter": "GamemodeOne.json-compact-prettier",
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "file"
    },
    "[jsonc]": { "editor.defaultFormatter": "GamemodeOne.json-compact-prettier" },
    "json-compact-prettier.maxLineLength": 140,
    "json-compact-prettier.alwaysOpenKeys": [ "alternates", "attendees", "permissions", "allow", "deny", "ask" ]
}