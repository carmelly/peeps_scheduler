{
    "version": "0.2.0",
    "inputs": [{ "id": "dataDir", "type": "promptString", "description": "Enter data-dir:" }],
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [ "run", "--data-folder", "${input:dataDir}" ],
            "console": "integratedTerminal"
        },
        {
            "name": "Run w/CSV Import",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [ "run", "--load-from-csv", "--data-folder", "${input:dataDir}" ],
            "console": "integratedTerminal"
        },
        {
            "name": "Availability Report",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [ "availability-report", "--data-folder", "${input:dataDir}" ],
            "console": "integratedTerminal"
        },
        {
            "name": "Apply Results",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [ "apply-results", "--period-folder", "${input:dataDir}" ],
            "console": "integratedTerminal"
        }
    ]
}