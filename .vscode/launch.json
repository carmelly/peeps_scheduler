{

	"version": "0.2.0",
	"inputs": [
        {
            "id": "dataDir",
            "type": "promptString",
            "description": "Enter data-dir:"
        }
    ],
	"configurations": [
		{
			"name":"Python Debugger: Current File",
			"type":"debugpy","request":"launch",
			"program":"${file}",
			"console":"integratedTerminal"
		},
		{
			"name": "Run",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/main.py",
			"args": ["run","--data-folder", "${input:dataDir}"],
			"console": "integratedTerminal",
		},
		{
			"name": "Run w/CSV Import",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/main.py",
			"args": ["run", "--load-from-csv", "--data-folder", "${input:dataDir}"],
			"console": "integratedTerminal", 
			// "env": {
			// 	"DATA_FOLDER": "2026-01"
			// }
		},
		{
			"name": "Apply Results",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/main.py",
			"args": ["run", "--data-folder", "2026-01"],
			"console": "integratedTerminal"
		}
	]
}
